dist: 
	@test -d .tmp/visualizer1.0.0 || mkdir -p .tmp/visualizer1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/visualizer1.0.0/ && (cd `dirname .tmp/visualizer1.0.0` && $(TAR) visualizer1.0.0.tar visualizer1.0.0 && $(COMPRESS) visualizer1.0.0.tar) && $(MOVE) `dirname .tmp/visualizer1.0.0`/visualizer1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/visualizer1.0.0


